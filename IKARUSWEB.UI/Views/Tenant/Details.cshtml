@model IKARUSWEB.UI.Models.TenantViewModels.TenantViewModel
@{
    ViewData["Title"] = "Tenant Details";
}

<h2>Tenant Details</h2>

<table class="table">
    <tr><th>Code</th><td>@Model.Code</td></tr>
    <tr><th>Name</th><td>@Model.Name</td></tr>
    <tr><th>Address</th><td>@Model.Address</td></tr>
    <tr><th>Phone</th><td>@Model.PhoneNumber</td></tr>
    <tr><th>Email</th><td>@Model.Email</td></tr>
    <tr><th>IsActive</th><td>@Model.IsActive</td></tr>
    <tr><th>CreatedDate</th><td>@Model.CreatedDate</td></tr>
</table>

<a asp-action="Create" class="btn btn-secondary">Back to Create</a>
